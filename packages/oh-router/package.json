    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0"
  },
  "dependencies": {
    "history": "^5.3.0",
    "oh-router-shared": "workspace:^"
  },
  "repository": "lblblong/oh-router",
  "homepage": "https://github.com/lblblong/oh-router"
