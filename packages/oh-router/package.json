{
  "name": "oh-router",
  "version": "0.3.1",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "types": "./dist/index.d.ts",
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.js",
    "exports": {
      ".": {
        "import": "./dist/index.js",
